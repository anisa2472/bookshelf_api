{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": "off",
        "no-console": "off",
        "no-tabs": 0,
        "consistent-return": "off",
        "no-unneeded-ternary": "off",
		"arrow-body-style": "off"
    }
}
